{
	"log": {
		"loglevel": "warning"
	},

	// 
	"inbounds": [
		{
			"tag": "inbound-http",
			"listen": "0.0.0.0",
			"port": 6010,
			"protocol": "http",
			"settings": {
				"timeout": 300,
				"accounts": [],
				"allowTransparent": false,
				"userLevel": 0
			},
      		"streamSettings": {}
    	},
    	{
			"tag": "inbound-socks",
			"listen": "0.0.0.0",
			"port": 6011,
			"protocol": "socks",
			"settings": {
				"auth": "noauth",
				"accounts": [],
				"udp": false,
				"ip": "127.0.0.1",
				"userLevel": 0
			},
			"streamSettings": {}
		}
  	],

	// 
	"outbounds": [
		{
			"tag": "outbound-freedom",
			"protocol": "freedom",
			"settings": {}
		},
		{
			"tag": "outbound-vmess",
			"protocol": "vmess",
			"settings": {
				"vnext": [
					{
						"address": "singapore.huawei.cloud.wengerbinning.xyz",
						"port": 443,
						"users": [
							{
								"id": "acbee383-cbeb-d947-c96a-86b807a02e26",
								"alterId": 0,
								"secirity": "auto",
								"level": 0
							}
						]
					}
				]
			},
			"streamSettings": {
				"network": "ws",
				"security": "tls",
				"wsSettings": {
					"acceptProtocol": false,
					"path": "/vpn/v2ray/vmess",
					"headers": {
						"Host": "singapore.huawei.cloud.wengerbinning.xyz"
					}
				}
			}
		},
		{
			"tag": "outbound-vless",
			"protocol": "vless",
			"settings": {
				"vnext": [{
					"address": "singapore.huawei.cloud.wengerbinning.xyz",
					"port": 443,
					"users": [{
						"id": "acbee383-cbeb-d947-c96a-86b807a02e26",
						"encryption": "none",
						"level": 0
					}]
				}]
			},
			"streamSettings": {
				"network": "ws",
				"security": "tls",
				"wsSettings": {
				"acceptProtocol": false,
				"path": "/vpn/v2ray/vless",
				"headers": {
					"Host": "singapore.huawei.cloud.wengerbinning.xyz"
				}
				}
			}
		},
		{
			"tag": "outbound-blackhole",
			"protocol": "blackhole",
			"settings": {
				"response": {
					"type": "http"
				}
			}
		}
	],

	// 
	"routing": {
		"domainStrategy": "IPIfNonMatch",
		"domainMatcher": "mph",
		"rules": [
			{
				"type": "field",
				"domains": [
					"geosite:category-ads-all"
				],
				"outboundTag": "outbound-blackhole"
			},
			{
				"type": "field",
				"domains": [
					"wireshark.org",
					"wengerbinning.xyz",
					"geosite:cn"
				],
				"outboundTag": "outbound-freedom"
			},
			{
				"type": "field",
				"ip": [
					"192.168.0.0/16",
					"172.16.0.0/16",
					"10.0.0.0/8"
				],
				"outboundTag": "outbound-freedom"
			},
			{
				"type": "field",
				"domains": [
					"github.com",
					"geosite:geolocation-!cn"
				],
				"outboundTag": "outbound-vless"
			}
		]
	}
}
